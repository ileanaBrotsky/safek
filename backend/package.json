{
  "name": "safekids-backend",
  "version": "1.0.0",
  "description": "SafeKids Backend API - Control Parental",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test-server": "node test-server.js",
    "test-db": "node test-database.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "node database/migrate.js",
    "seed": "node database/seed.js",
    "db:setup": "npm run migrate && npm run seed",
    "install-deps": "npm install",
    "docker:init": "node scripts/init-docker.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart backend",
    "docker:clean": "docker-compose down -v && docker system prune -f"
  },
  "keywords": [
    "parental-control",
    "safety",
    "monitoring",
    "children"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}